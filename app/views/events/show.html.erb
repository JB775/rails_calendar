<p id="notice"><%= notice %></p>

<%= image_tag @event.image.url() %>

<p>
  <strong>Title:</strong>
  <%= @event.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @event.description %>
</p>

<p>
  <strong>Dateandtime:</strong>
  <%= @event.dateandtime %>
</p>

<p>
  <strong>User:</strong>
  <%= @event.user_id %>
</p>

<p>
  <strong>Attendees:</strong>
</p>

<% @attendeeArray.each do |t| %>
  <p>
    <%= t %>
  </p>
<% end %>

<br>
<br>
<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
